<!DOCTYPE html>
<html>
<head>
  <title>MaketStorage</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_page?(maket_path) %>
	<%= yield %>
<% else %>
	<div class="container">
		<div class="header">
			<h3 class="text-muted">Maket storage</h3>
		</div>
		<%= yield %>
	</div>
<% end %>
</body>
</html>
