<div class="jumbotron">
	<h3>Версии</h3>
	<%= render 'versions/add_version', :version => @project.versions.build %>
	<table class="table table-hover versions">
		<thead>
			<tr>
				<th>Название</th>
			</tr>
		</thead>
		<%= render :partial => "versions/version", :collection => @project.versions.order("created_at ASC"), :as => 'version' %>
	</table>
</div>
<%= link_to "Вернуться в списку проектов", projects_path %>