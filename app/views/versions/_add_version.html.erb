<%= form_for [version.project, version], :html => { class:"form-inline", role:"form" } do |f| %>
	<%= f.text_field "name", placeholder: "Version tag", class: "form-control"%>
	<%= f.hidden_field :project_id, :value => version.project_id %>

	<%= f.submit "Add Version", class: "btn btn-success btn-sm" %>
<% end %>